{
  "expo": {
    "name": "FarmNamin",
    "slug": "FarmNamin",
    "scheme": "farmnamin",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#4CAF50"
    },
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "version": "1.0.2",
      "bundleIdentifier": "com.jamesdavid.FarmNamin"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.jamesdavid.FarmNamin"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": ["expo-video", "expo-router"],
    "extra": {
      "eas": {
        "projectId": "f1ad38e8-be95-4c72-ad41-ab60ffc895fa"
      }
    }
  }
}
